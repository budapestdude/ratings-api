{
  "name": "fide-rating-api",
  "version": "1.0.0",
  "description": "API for accessing FIDE chess ratings",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "start:production": "NODE_ENV=production node dist/index.js",
    "start:render": "NODE_ENV=production node dist/index.js",
    "build:all": "./build.sh",
    "init-sample-data": "tsx src/scripts/init-sample-data.ts",
    "import-ratings": "tsx src/scripts/import-all-ratings.ts",
    "import-rapid-blitz": "tsx src/scripts/import-all-ratings.ts",
    "update-ratings": "tsx src/scripts/update-ratings.ts",
    "generate-top100": "tsx src/scripts/generate-top100.ts",
    "monthly-update": "tsx src/scripts/monthly-update.ts",
    "monthly-update:now": "tsx src/scripts/monthly-update.ts --run-now",
    "migrate:postgres": "tsx src/database/migrate.ts",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@types/pg": "^8.15.5",
    "axios": "^1.6.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "node-cron": "^3.0.3",
    "pg": "^8.16.3",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.6",
    "unzipper": "^0.10.14",
    "xlsx": "^0.18.5",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/unzipper": "^0.10.9",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "tsx": "^4.20.5",
    "typescript": "^5.3.3"
  }
}
